{
  "poetryMonorepo.appendExtraPaths": true,
  "python.analysis.extraPaths": [
    "packages/langchain-auth0-ai/langchain_auth0_ai",
    "packages/auth0-ai/auth0_ai",
    "packages/llama-index-auth0-ai/tests",
    "packages/langchain-auth0-ai/tests",
    "packages/llama-index-auth0-ai/llama_index_auth0_ai",
    "examples/authorization-for-rag/langchain-examples/langchain_rag",
    "examples/authorization-for-rag/llama-index-examples/llama_index_rag"
  ],
  "python.testing.pytestArgs": [
    "packages/langchain-auth0-ai/tests",
    "packages/llama-index-auth0-ai/tests"
  ],
  "python.testing.unittestEnabled": false,
  "python.testing.pytestEnabled": true,
  "editor.tabSize": 4,
  "editor.formatOnSave": true,
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "always",
      "source.unusedImports": "always",
      "source.fixAll": "always"
    },
    "editor.defaultFormatter": "ms-python.autopep8"
  }
}
